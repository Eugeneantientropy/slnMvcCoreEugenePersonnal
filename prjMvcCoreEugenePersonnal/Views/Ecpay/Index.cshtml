@model Dictionary<string, string>

<!DOCTYPE html>
<html>
<head>
    <title>Auto Submit Form</title>
</head>
<body onload="document.forms['autoSubmitForm'].submit();">

    <form id="autoSubmitForm" name="autoSubmitForm" method="POST" action="https://payment-stage.ecpay.com.tw/Cashier/AioCheckOut/V5">
        @foreach (var key in Model.Keys)
        {
            <input type="hidden" name="@key" value="@Model[key]" />
            <br />
        }
    </form>

</body>
</html>